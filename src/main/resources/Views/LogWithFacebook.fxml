   <children>
      <Button fx:id="login_as_facebook" layoutX="238.0" layoutY="287.0" mnemonicParsing="false" onAction="#login_as_facebook" text="Login with facebook" />
      <Circle fx:id="profile_pic" fill="#1f93ff" layoutX="300.0" layoutY="114.0" radius="100.0" stroke="WHITE" strokeType="INSIDE" />
      <Label fx:id="name" layoutX="198.0" layoutY="239.0" prefHeight="18.0" prefWidth="230.0" text="Full Name" textFill="WHITE" />
      <Button fx:id="gotoSignup" layoutX="192.0" layoutY="344.0" mnemonicParsing="false" onAction="#gotoSignup" prefHeight="37.0" prefWidth="216.0" style="-fx-background-color: #3b5998; -fx-border-color: #fff; -fx-border-radius: 10;" text="Terminer votre inscription" textFill="WHITE" />
   </children>
</AnchorPane>
